{
  "name": "vue-pandora",
  "version": "1.0.0",
  "description": "A component library of custom secondary encapsulation about element-ui",
  "main": "src/index.ts",
  "repository": "https://github.com/yelingfeng/vue-pandora.git",
  "author": "yelngfeng <yelingfeng521@gmail.com>",
  "license": " MIT",
  "scripts": {
    "dev": "vue-cli-service serve --open",
    "build": "vue-cli-service build ",
    "lib": "vue-cli-service build --target lib --name vuepandora src/index.ts",
    "lint": "vue-cli-service lint --fix",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "commit": "git-cz",
    "publish": "npm publish"
  },
  "files": [
    "lib",
    "src",
    "*.d.ts"
  ],
  "dependencies": {
    "core-js": "^3.6.4",
    "dayjs": "^1.8.23",
    "element-ui": "^2.13.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.149",
    "@types/mockjs": "^1.0.2",
    "@types/node": "^13.9.1",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-typescript": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.1",
    "@vuepress/plugin-back-to-top": "^1.3.1",
    "axios": "^0.19.2",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.1.2",
    "husky": "^4.2.3",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.0.8",
    "lodash": "^4.17.15",
    "mockjs": "^1.1.0",
    "prettier": "^1.19.1",
    "typescript": "~3.7.5",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-property-decorator": "^8.3.0",
    "vue-template-compiler": "^2.6.11",
    "yarn": "^1.22.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts?(x),json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
